extends layout



block content   
	if (req.query.id)
		h1 You are on project #{req.query.id}
	else 
		h1 You are on project 1

	#track_container
		#master_control
			include ./master_jplayer.html
		ul#track_type_list
			li.track_type
				h1 Vocals
				ul.track_list
					li #{req.query.vocals1}
						include ./slave_jplayer.html
					li #{req.query.vocals2}
			li.track_type 
				h1 Guitar
				ul.track_list
					li #{req.query.guitar1}
					li #{req.query.guitar2}
			li.track_type 
				h1 Percussion
				ul.track_list
					li #{req.query.percussion1}
					li #{req.query.percussion2}
			li.track_type 
				h1 Wind
				ul.track_list
					li #{req.query.wind1}
					li #{req.query.wind2}