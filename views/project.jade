extends layout

#{vocals[], guitar[], percussion[], wind[]}

block content   
	h1 You are on project 1

	#track_container.track_container
		#master_control
			include ./master_jplayer.jade
		ul#track_type_list
			li.track_type#left-track
				h1 Vocals
				ul.track_list
					li #{req.query.vocals1}
						include ./slave_jplayerV.jade
					li #{req.query.vocals2}
			li.track_type 
				h1 Guitar
				ul.track_list
					li #{req.query.guitar1}
						include ./slave_jplayerG.jade
					li #{req.query.guitar2}
			li.track_type 
				h1 Percussion
				ul.track_list
					li #{req.query.percussion1}
						include ./slave_jplayerP.jade
					li #{req.query.percussion2}
			li.track_type 
				h1 Wind
				ul.track_list
					li #{req.query.wind1}
						include ./slave_jplayerW.jade
					li #{req.query.wind2}